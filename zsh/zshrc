ZSH=$HOME/.oh-my-zsh
ZSH_THEME="gary"
COMPLETION_WAITING_DOTS="true"

plugins=(command-coloring git zsh-syntax-highlighting brew osx heroku gem cap ruby rvm git-flow eifion vagrant battery themes git-hubflow safe-paste zeus zsh-syntax-highlighting git-extras)

# autoload bashcompinit
# bashcompinit

source $ZSH/oh-my-zsh.sh
for f ($HOME/.zsh/*.zsh) {source $f}
for f ($HOME/.zsh/private/*.zsh) {source $f}

if [[ $IS_MAC -eq 1 ]]; then
  [[ -s "$HOME/.rvm/scripts/rvm" ]] && . "$HOME/.rvm/scripts/rvm"
  source /usr/local/share/zsh-syntax-highlighting/zsh-syntax-highlighting.zsh
  ZSH_HIGHLIGHT_HIGHLIGHTERS=(main brackets pattern cursor)
fi

if [[ $IS_LINUX -eq 1 ]]; then
  # [[ -s "$HOME/.rvm/scripts/rvm" ]] && . "$HOME/.rvm/scripts/rvm"
fi

uptime | cowsay -f stegosaurus
echo ""
